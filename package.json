{
  "name": "ch_meetings_server",
  "version": "1.0.0",
  "main": "app.ts",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "start": "node dist/app.js",
    "dev": "nodemon dist/app.js",
    "watch": "tsc -w",
    "docker:build": "docker-compose -f docker-compose.yml build --no-cache",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:migrate": "docker-compose -f docker-compose.yml run --rm test_server npx prisma migrate dev --name init",
    "seed": "prisma db seed --preview-feature",
    "docker:seed": "docker-compose -f docker-compose.yml run --rm test_server yarn prisma db seed --preview-feature",
    "test": "mocha  --timeout 15000 -r ts-node/register --require tests/mochaHooks.ts 'tests/**/*.ts'",
    "docker:test": " docker-compose -f docker-compose.yml run --rm test_server yarn mocha  --timeout 15000 -r ts-node/register --require tests/mochaHooks.ts 'tests/**/*.ts'",
    "docker:prep": "yarn docker:up && yarn docker:migrate && yarn docker:test",
    "docker:redis-local": "docker run --name redis_ch_meetings -p 6379:6379 -d redis"
  },
  "devDependencies": {
    "@types/argon2": "^0.15.0",
    "@types/chai": "^4.2.18",
    "@types/connect-redis": "^0.0.16",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/express-session": "^1.17.3",
    "@types/ioredis": "^4.26.0",
    "@types/luxon": "^1.27.0",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.0.2",
    "@types/nodemailer": "^6.4.2",
    "@types/pg": "^7.14.11",
    "@types/uuid": "^8.3.0",
    "axios": "^0.21.1",
    "chai": "^4.3.4",
    "mocha": "^8.4.0",
    "nodemon": "^2.0.7",
    "prisma": "^2.23.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@prisma/client": "^2.23.0",
    "apollo-server-express": "^2.24.0",
    "argon2": "^0.27.2",
    "connect-redis": "^5.2.0",
    "cros": "^1.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "graphql": "^15.5.0",
    "graphql-query-complexity": "^0.8.0",
    "ioredis": "^4.27.1",
    "luxon": "^1.27.0",
    "nodemailer": "^6.6.2",
    "pg": "^8.6.0",
    "reflect-metadata": "^0.1.13",
    "type-graphql": "^1.1.1",
    "uuid": "^8.3.2"
  }
}
